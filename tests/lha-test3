							check $?
$lha pq test-tmp-m5g.lzh test-a | diff test-a -
							check $?
$lha pq test-tmp-m6g.lzh test-a | diff test-a -
							check $?
$lha pq test-tmp-m7g.lzh test-a | diff test-a -
							check $?

cat test-[abc] > test-tmp-abc
